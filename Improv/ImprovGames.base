views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("Improv")
        - file.name != "ImprovGames.base"
    order:
      - file.name
      - Description
      - Players
      - Category
      - Type
      - Played
    sort:
      - property: file.name
        direction: DESC
    summaries:
      Category: Filled
    columnSize:
      note.Description: 596
  - type: cards
    name: View
